            android:name="vector"
            android:width="344dp"
            android:height="337dp"
            android:viewportWidth="360"
            android:viewportHeight="340">
            <path
                android:name="done"
                android:pathData="M 15.442 159.799 L 115.4 271.057 L 330.962 69.402"
                android:strokeColor="@color/dark_black"
